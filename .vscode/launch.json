{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/React1-Backend/bin/Debug/net8.0/React1-Backend.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ADMIN_TOKEN": "ADMIN_TOKEN",
        "ALIBABA_ACCESS_KEY": "ALIBABA_ACCESS_KEY",
        "ALIBABA_SECRET_KEY": "ALIBABA_SECRET_KEY",
        "AWS_ACCESS_KEY": "AWS_ACCESS_KEY",
        "AWS_SECRET_KEY": "AWS_SECRET_KEY",
        "MAILGUN_API_KEY": "MAILGUN_API_KEY",
        "MAILGUN_DOMAIN": "MAILGUN_DOMAIN",
        "MONGO_CONNECTION_STRING": "mongodb://localhost:27017",
        "MONGO_CONNECTION_STRING_INTTEST": "mongodb://localhost:27017",
        "PAYPAL_CLIENT_ID": "PAYPAL_CLIENT_ID",
        "PAYPAL_SECRET_KEY": "PAYPAL_SECRET_KEY",
        "USER_TOKEN": "USER_TOKEN",
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
