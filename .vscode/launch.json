  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net8.0/React1-backend.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "awsAccessKey": "x",
        "awsSecretKey": "x",
        "alibabaAccessKey": "x",
        "alibabaSecretKey": "x",
        "mongoConnectionString": "mongodb://localhost:27017",
        "adminToken": "x",
        "userToken": "x",
        "paypalClientId": "x",
        "paypalSecretKey": "x"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}